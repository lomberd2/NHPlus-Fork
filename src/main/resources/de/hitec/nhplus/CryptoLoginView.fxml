<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.hitec.nhplus.controller.CryptoLoginController">
   <center>
      <VBox fx:id="MainVBox" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="labelTag" text="labelTag">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin></Label>
            <PasswordField fx:id="passwordBox">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin></PasswordField>
            <Button fx:id="submitButton" mnemonicParsing="false" onAction="#handleSubmitButton" text="Submit Password">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
   </center>
   <top>
      <Label fx:id="TitleLabel" text="TitleLabel" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
